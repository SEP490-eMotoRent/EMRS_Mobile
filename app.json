{
  "expo": {
    "scheme": "emrs",
    "name": "eMotoRent_Mobile",
    "slug": "eMotoRent_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phuuthanh2003.eMotoRent-Mobile",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_API_KEY_HERE"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "eMotoRent cần truy cập vị trí của bạn để tìm chi nhánh gần nhất và hiển thị xe có sẵn xung quanh bạn.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "eMotoRent cần truy cập vị trí của bạn để tìm chi nhánh gần nhất và hiển thị xe có sẵn xung quanh bạn.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["emrs"]
          }
        ],
        "NSLocationWhenInUseUsageDescription": "eMotoRent cần truy cập vị trí để hiển thị bản đồ và tìm xe gần bạn.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "eMotoRent cần truy cập vị trí để hiển thị bản đồ và tìm xe gần bạn."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "NOTHING_HERE"
        }
      },
      "package": "com.phuuthanh2003.eMotoRent_Mobile",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "emrs",
              "host": "payment",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "eMotoRent cần truy cập vị trí của bạn để tìm chi nhánh gần nhất và hiển thị xe có sẵn xung quanh bạn."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsVersion": "10.16.4"
        }
      ]
    ]
  }
}